import java.util.Arrays;
import java.util.List;

public void readCSV() throws IOException {

    String csv = vars.get(&quot;LOGIN&quot;) + &quot;.csv&quot;;
    FileReader fileReader = new FileReader(csv);
    String line = new BufferedReader(fileReader).readLine();
    List nums = Arrays.asList(line.split(&quot;,&quot;));
    vars.put(&quot;POST_NUM&quot;, nums.get(Integer.parseInt(vars.get(&quot;COUNTER&quot;))).toString());
    fileReader.close();
}

readCSV(); // here is the entry point

char SEPARATOR = &apos;,&apos;;

//function write line in csv
public void writeLine(String filename) {
    File file = new File(filename);
    FileWriter writer = new FileWriter(filename, true);
    if (file.length() != 0) {
        writer.append(SEPARATOR);
    }
    
    writer.append(vars.get(&quot;COUNTER&quot;));
    
    writer.flush();
    writer.close();
}
String csvFile = vars.get(&quot;LOGIN&quot;) + &quot;.csv&quot;;

writeLine(csvFile);

